<% include partials/header %>

<h1><%= photo.name %></h1>

<img src="<%= photo.image %>" alt="<%= photo.name %>">

<p><%= photo.description %></p>

<% photo.comments.forEach(function(comment){ %>
    <p><strong><%= comment.author %></strong> - <%= comment.text %></p>
<% }); %>

<% include partials/footer %>